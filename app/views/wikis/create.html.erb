<h1>Wikis#create</h1>
<p>Find me in app/views/wikis/create.html.erb</p>


<div>
    <h3>Create a new wiki</h3>
      <%= form_for @wiki, url: wikis_path(@wiki) do |f| %>
      <%= f.label :title %>: <%= f.text_field :title, autofocus: true %>
      <%= f.label :body %>: <%= f.text_field :body %>
      <%= f.submit "Button Title", class: 'btn btn-success' %>
      <% end %>
  </div>